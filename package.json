{
  "name": "@packt/ssm-helper",
  "version": "0.0.1",
  "description": "Module and CLI tool to help handle SSM variables",
  "main": "dist/index.js",
  "bin": {
    "ssmexport": "bin/export.bin.js"
  },
  "scripts": {
    "build": "babel src -d dist",
    "test": "npm run lint && npm run unit && npm run coverage",
    "lint": "eslint ./src",
    "coverage": "babel-node ./node_modules/babel-istanbul/lib/cli.js check-coverage --statements 95 --functions 95 --branches 95 --lines 95",
    "unit": "babel-node ./node_modules/babel-istanbul/lib/cli.js cover ./node_modules/mocha/bin/_mocha -- test/*.js",
    "prepublishOnly": "npm run test && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git@bitbucket.org:packt-internal/ssm-helper.git"
  },
  "author": {
    "name": "Packt Publishing Ltd",
    "url": "https://www.packtpub.com/"
  },
  "contributors": [
    {
      "name": "Mike Elsmore",
      "email": "mikee@packt.com"
    }
  ],
  "license": "Apache-2.0",
  "homepage": "https://bitbucket.org/packt-internal/ssm-helper",
  "devDependencies": {
    "aws-sdk-mock": "4.1.0",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-istanbul": "0.12.2",
    "babel-loader": "7.1.4",
    "babel-preset-env": "1.7.0",
    "chai": "4.1.2",
    "chai-as-promised": "7.1.1",
    "chai-bytes": "0.1.1",
    "eslint": "4.19.1",
    "eslint-config-airbnb-base": "12.1.0",
    "eslint-plugin-import": "2.12.0",
    "istanbul": "0.4.5",
    "mocha": "5.1.1",
    "mocha-bamboo-reporter": "1.1.1",
    "sinon": "4.5.0",
    "uuid": "3.3.2",
    "webpack": "4.12.0",
    "webpack-cli": "3.0.8"
  },
  "dependencies": {
    "@packt/error-custom": "0.1.2",
    "aws-sdk": "2.318.0",
    "commander": "2.20.0",
    "debug": "4.0.1",
    "joi": "13.6.0"
  }
}
